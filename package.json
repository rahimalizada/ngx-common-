    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@angular/localize": "^12.2.2",
    "@types/jasmine": "~3.8.2",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "eslint": "^7.32.0",

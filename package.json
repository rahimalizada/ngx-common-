{
  "name": "ngx-common",
  "version": "0.1.52",
  "description": "Common library. Includes base model files. includes a base abstract service for REST services and related angular resolvers/interceptors. Includes generic JWT authentication services. Includes external script loader for angular projects. Includes a collection of Angular pipes. Includes a collection of Angular validators. Includes an Angular interceptor that adds a launch mode header to PWAs. Includes shared components for working with angular forms. Includes a directive for enabling/disabling an element indicating http/navigation progress.",
  "keywords": [
    "common",
    "lib",
    "angular",
    "rest",
    "http",
    "service",
    "resolver",
    "interceptor",
    "jwt",
    "authentication",
    "external",
    "script",
    "loader",
    "javascript",
    "pipe",
    "validator",
    "interceptor",
    "pwa",
    "launch",
    "header",
    "mode",
    "form",
    "reactive",
    "navigation",
    "progress",
    "indicator"
  ],
  "author": {
    "name": "Rahim Alizada"
  },
  "license": "MIT",
  "homepage": "https://github.com/rahimalizada/ngx-common",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahimalizada/ngx-common.git"
  },
  "bugs": {
    "url": "https://github.com/rahimalizada/ngx-common/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "xi18n": "ts-node -- ./projects/rahimalizada/ngx-common/src/lib/i18n/translation-merger.ts",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "_build": "ng build --configuration=production && npm run xi18n",
    "_xi18n": "localize-extract -r . -s \"./dist/rahimalizada/ngx-common/fesm2015/*.js\" -f xlf2 -o ./projects/rahimalizada/ngx-common/src/assets/i18n/messages.xlf"
  },
  "private": true,
